APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Registrar Configuration
REGISTRAR_TIMEOUT=30
REGISTRAR_ENABLE_LOGGING=true
REGISTRAR_RATE_LIMIT_ATTEMPTS=60
REGISTRAR_RATE_LIMIT_DECAY=1
REGISTRAR_CACHE_TTL=300

# ResellerClub/LogicBoxes Configuration
RESELLERCLUB_API_URL=https://httpapi.com/api
RESELLERCLUB_TEST_MODE=false
RESELLERCLUB_AUTH_USERID=
RESELLERCLUB_API_KEY=
RESELLERCLUB_NS1=ns1.resellerclub.com
RESELLERCLUB_NS2=ns2.resellerclub.com

# LogicBoxes Configuration (same API as ResellerClub)
LOGICBOXES_API_URL=https://httpapi.com/api
LOGICBOXES_TEST_MODE=false
LOGICBOXES_AUTH_USERID=
LOGICBOXES_API_KEY=

# Mock Registrar (for testing)
MOCK_SIMULATE_DELAYS=false
MOCK_DELAY_MS=100
MOCK_FAILURE_RATE=0
MOCK_TRACK_HISTORY=true
